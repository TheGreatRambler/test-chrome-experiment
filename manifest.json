{
  "manifest_version": 2,

  "name": "Reddit post updater",
  "description": "Gives you a new reddit post whenever you need one",
  "version": "1.0",

  "browser_action": {
   "default_popup": "popup.html"
  },
  
  "background": [
    {
      "scripts": ["jquery.js", "content.js"]
    }
  ],
  
  "permissions": [
   "activeTab"
   ]
}
