{
  "manifest_version": 2,

  "name": "Ad clicker",
  "description": "This extension will do something... not sure what yet",
  "version": "1.0",

  "browser_action": {
   "default_popup": "popup.html"
  },
  
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ],
  
  "permissions": [
   "activeTab"
   ]
}
